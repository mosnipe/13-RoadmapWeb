{
  "name": "daikon",
  "version": "1.0.0",
  "description": "リリースノート＆開発ロードマップ Webアプリ",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss ./src/css/tailwind.css -o ./dist/tailwind.css --minify",
    "watch:css": "postcss ./src/css/tailwind.css -o ./dist/tailwind.css --watch",
    "dev": "npm run build:css && npx http-server . -p 8080 -o",
    "start": "npm run build:css && npx http-server . -p 8080 -o"
  },
  "keywords": [
    "roadmap",
    "release-notes",
    "github",
    "timeline"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.19"
  }
}
